             xmlns:vms="clr-namespace:PlatinumGymPro.ViewModels.PlayersViewModels"
             xmlns:vmodels="clr-namespace:PlatinumGymPro.ViewModels.Accountant"
             xmlns:views="clr-namespace:PlatinumGymPro.Views"
             xmlns:vs="clr-namespace:PlatinumGymPro.Views.AccountingViews"
             xmlns:playervs="clr-namespace:PlatinumGymPro.Views.PlayerViews"
             xmlns:vi="clr-namespace:PlatinumGymPro.Views.PlayerViews"
