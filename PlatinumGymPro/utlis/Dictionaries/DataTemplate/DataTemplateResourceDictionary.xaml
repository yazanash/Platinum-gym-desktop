<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    
                      xmlns:viewmodels="clr-namespace:PlatinumGymPro.ViewModels"
             xmlns:vms="clr-namespace:PlatinumGymPro.ViewModels.PlayersViewModels"
             xmlns:vmodels="clr-namespace:PlatinumGymPro.ViewModels.Accountant"
             xmlns:views="clr-namespace:PlatinumGymPro.Views"
             xmlns:vs="clr-namespace:PlatinumGymPro.Views.AccountingViews"
             xmlns:playervs="clr-namespace:PlatinumGymPro.Views.PlayerViews"
             xmlns:vi="clr-namespace:PlatinumGymPro.Views.PlayerViews"
             xmlns:login="clr-namespace:PlatinumGymPro.Views.AuthView"
                       xmlns:routinevms="clr-namespace:PlatinumGymPro.ViewModels.RoutineViewModels"
             xmlns:routineview="clr-namespace:PlatinumGymPro.Views.RoutineViews"
                    
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate DataType="{x:Type viewmodels:MainViewModel}">
        <views:MainView />

    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:HomeViewModel }">
        <views:HomeView />

    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AuthViewModel }">
        <!--<views:HomeView />-->
        <login:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:SportsViewModel }">
        <views:SportsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:TrainersViewModel}">
        <views:TrainersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vmodels:AccountingViewModel }">
        <vs:AccountnigView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:PlayersPageViewModel }">
        <views:PlayersView/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type routinevms:RoutinePlayerViewModels}">
        <routineview:RoutineView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type routinevms:AddRoutineViewModel}">
        <routineview:AddRoutineView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type routinevms:EditRoutineViewModel}">
        <routineview:EditRoutineView />
    </DataTemplate>
</ResourceDictionary>